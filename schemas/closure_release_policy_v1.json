{
  "policy_version": "closure_release_policy_v1",
  "default_mode": "attach",
  "supported_modes": [
    "off",
    "attach",
    "warn",
    "enforce"
  ],
  "classifier_version": "1",
  "score_threshold": 0.85,
  "minimum_readiness_runs": 14,
