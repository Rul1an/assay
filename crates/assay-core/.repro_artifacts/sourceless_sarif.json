{
  "$schema": "https://raw.githubusercontent.com/oasis-tcs/sarif-spec/main/sarif-2.1/schema/sarif-schema-2.1.0.json",
  "runs": [
    {
      "results": [
        {
          "level": "error",
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": ".assay/eval.yaml"
                },
                "region": {
                  "startColumn": 1,
                  "startLine": 1
                }
              }
            }
          ],
          "message": {
            "text": "sourceless_failure_demo: Config error or internal logic failure without file context"
          },
          "ruleId": "assay"
        }
      ],
      "tool": {
        "driver": {
          "name": "assay"
        }
      }
    }
  ],
  "version": "2.1.0"
}
